#!/usr/bin/env cosh

yaegi extract fmt strconv strings image image/color image/draw time log/slog reflect

command extract {
    for _, pkg := range args {
        yaegi extract {"cogentcore.org/core/"+pkg}
    }
}

extract core icons events styles styles/states styles/abilities styles/units tree keymap colors colors/gradient filetree texteditor htmlcore pages paint math32 plot plot/plots plot/plotcore tensor/table base/errors base/fsx base/reflectx base/labels base/fileinfo
