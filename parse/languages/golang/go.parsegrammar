    Consts:  'key:const' ConstDeclN 'EOS'  >AST
    // Types same as TypeDecl 
    Types:  'key:type' TypeDeclN 'EOS'  >AST
    // Vars same as VarDecl 
    Vars:   'key:var' VarDeclN 'EOS'  >AST
